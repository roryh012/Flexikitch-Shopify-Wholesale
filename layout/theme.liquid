  <script>
    {% comment %} Wholesale class check on body load {% endcomment %}
    try{
      function getCookie(name) {
        const value = `; ${document.cookie}`;
        const parts = value.split(`; ${name}=`);
        if (parts.length === 2) return parts.pop().split(';').shift();
      }
      const siteState = getCookie('siteState');
      const isWholesaleCustomer = '{% if customer and customer.tags contains "wholesale" %}yes{% else %}no{% endif %}'
      if (siteState === 'wholesale' && isWholesaleCustomer === 'yes') {
        document.body.classList.add('wholesale-state');
      }
    }catch(e){
      console.error(e);
    }
  </script>
