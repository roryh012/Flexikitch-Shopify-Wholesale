{% comment %}
    Automatically generated by Konigle. DO NOT EDIT THIS FILE
{% endcomment %}
<div class="page-width">
    <div class="grid">
        <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
            <div class="section-header text-center">
                <h1>{{ page.title }}</h1>
            </div>
            <div class="rte">
                {%- assign collection = collections['all'] -%}
                {% paginate collection.products by 250 %}
                    {% for product in collection.products %}
                        <a href="/products/{{ product.handle }}" target="_blank">{{ product.title }}</a><br>
                    {% endfor %}
                    {%- if paginate.pages > 1 -%}
                        {% include 'pagination', paginate: paginate %}
                    {%- endif -%}
                {% endpaginate %}
            </div>

        </div>
    </div>
    <div style="text-align: right; font-size: 12px; color: #828080; margin-top: 3rem;">
        Powered by <a target="_blank" style="color: #1C4ADB" href="https://konigle.com">konigle</a>
    </div>
</div>