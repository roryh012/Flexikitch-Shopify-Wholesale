    <script type="text/javascript">
        function openWeekly(){
            document.getElementById('capital-cost').classList.remove('active')
            document.getElementById('weekly-cost').classList.add('active')
            document.querySelector('.tab-capital').classList.remove('active')
            document.querySelector('.tab-weekly').classList.add('active')
        }
        function openCapital(){
            document.getElementById('capital-cost').classList.add('active')
            document.getElementById('weekly-cost').classList.remove('active')
            document.querySelector('.tab-capital').classList.add('active')
            document.querySelector('.tab-weekly').classList.remove('active')
        }
        function calculateCost(method){
            try {
                var price = 0;
                var weekly = 0;
                var deposit = 0;
                var buyout = 0;
                var rwm = '0.0545';
                var rwd = '4.33';
                var rwsm = '';
                var rwsd = '';
                var rdp = '10';
                var rbp = '60';
                var currencyFormatter = new Intl.NumberFormat('en-US', {
                    style: 'currency',
                    currency: 'USD',
                });
                if(!isNaN(parseFloat(price))){
                    price = document.getElementById(method+'-input').value;
                    if( method === "capital-cost"){
                        weekly = currencyFormatter.format((rwm * price) / rwd).replace('$','');
                        deposit = currencyFormatter.format((price * rdp) / 100).replace('$','');
                        buyout = currencyFormatter.format((price * rbp) / 100).replace('$','');
                    } else{
                        var weeklyRaw = (rwd * price) / rwm
                        weekly = currencyFormatter.format(weeklyRaw).replace('$','')
                        deposit = currencyFormatter.format((weeklyRaw * rdp) / 100).replace('$','');
                        buyout = currencyFormatter.format((weeklyRaw * rbp) / 100).replace('$','');
                    }
                    document.getElementById('weekly-'+method).innerHTML = weekly
                    document.getElementById('deposit-'+method).innerHTML = deposit
                    document.getElementById('buyout-'+method).innerHTML = buyout
                } else{
                    alert('Cost is invalid number.');
                }
            } catch (error) {}
        }

    </script>
