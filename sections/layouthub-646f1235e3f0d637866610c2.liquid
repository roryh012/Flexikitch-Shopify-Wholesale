<h3 class="lh-title" ><strong>Finance Calculator</strong></h3>
<div class="calculation-top">
    <div class="calculation-tabs">
        <div class="tabs-header visiable-desktop">
            <div class="tabs-title tab-capital active">
                <button class="fktab" onclick="openCapital()">Calculate by Capital Cost</button>
            </div>
            <div class="tabs-title tab-weekly">
                <button class="fktab" onclick="openWeekly()">Calculate by Weekly Rental</button>
            </div>
        </div>
        <div class="calculation-tabs-content">
            <div class="visiable-mobile tabs-title active">
                <a href="javascript: void(0)" class="capital-cost-mb" data-tab="capital-cost">Calculate by Capital Cost</a>
            </div>
            <div id="capital-cost" class="calculation-tabs-item active">
                <div class="calculation-calculate">
                    <label>Capital cost</label>
                    <div class="control">
                        <span>$</span><input name="text" id="capital-cost-input" value="" placeholder="0.00">
                    </div>
                    <button type="button" title="Calculate" class="action calculate-capital" onclick="calculateCost('capital-cost')">
                        <span>Calculate</span>
                    </button>
                </div>
                <div class="calculation-totals">
                    <table>
                        <tbody>
                            <tr>
                                <th>Weekly cost</th>
                                <td><span class="total-currency">$</span><span id="weekly-capital-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Deposit (10%)</th>
                                <td><span class="total-currency">$</span><span id="deposit-capital-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Buy out (1st year)</th>
                                <td><span class="total-currency">$</span><span id="buyout-capital-cost">0.00</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="visiable-mobile tabs-title"><a href="javascript: void(0)" class="weekly-cost-mb" data-tab="weekly-cost">Calculate by Weekly Rental</a></div>
            <div id="weekly-cost" class="calculation-tabs-item">
                <div class="calculation-calculate">
                    <label>Weekly cost</label>
                    <div class="control">
                        <span>$</span><input name="text" id="weekly-cost-input" value="" placeholder="0.00">
                    </div>
                    <button type="button" title="Calculate" class="action calculate-weekly" onclick="calculateCost('weekly-cost')">
                        <span>Calculate</span>
                    </button>
                </div>
                <div class="calculation-totals">
                    <table>
                        <tbody>
                            <tr>
                                <th>Capital cost</th>
                                <td><span class="total-currency">$</span><span id="weekly-weekly-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Deposit (10%)</th>
                                <td><span class="total-currency">$</span><span id="deposit-weekly-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Buy out (1st year)</th>
                                <td><span class="total-currency">$</span><span id="buyout-weekly-cost">0.00</span></td>
