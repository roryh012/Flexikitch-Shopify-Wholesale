                    <table>
                        <tbody>
                            <tr>
                                <th>Weekly cost</th>
                                <td><span class="total-currency">$</span><span id="weekly-capital-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Deposit (10%)</th>
                                <td><span class="total-currency">$</span><span id="deposit-capital-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Buy out (1st year)</th>
                                <td><span class="total-currency">$</span><span id="buyout-capital-cost">0.00</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="visiable-mobile tabs-title"><a href="javascript: void(0)" class="weekly-cost-mb" data-tab="weekly-cost">Calculate by Weekly Rental</a></div>
            <div id="weekly-cost" class="calculation-tabs-item">
                <div class="calculation-calculate">
                    <label>Weekly cost</label>
                    <div class="control">
                        <span>$</span><input name="text" id="weekly-cost-input" value="" placeholder="0.00">
                    </div>
                    <button type="button" title="Calculate" class="action calculate-weekly" onclick="calculateCost('weekly-cost')">
                        <span>Calculate</span>
                    </button>
                </div>
                <div class="calculation-totals">
                    <table>
                        <tbody>
                            <tr>
                                <th>Capital cost</th>
                                <td><span class="total-currency">$</span><span id="weekly-weekly-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Deposit (10%)</th>
                                <td><span class="total-currency">$</span><span id="deposit-weekly-cost">0.00</span></td>
                            </tr>
                            <tr>
                                <th>Buy out (1st year)</th>
                                <td><span class="total-currency">$</span><span id="buyout-weekly-cost">0.00</span></td>
