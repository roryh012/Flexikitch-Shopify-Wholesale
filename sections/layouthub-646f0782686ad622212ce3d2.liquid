<div class="calculation-top">
    <div class="calculation-tabs">
        <div class="tabs-header visiable-desktop">
            <div class="tabs-title tab-capital active">
                <button class="fktab" onclick="openCapital()">Calculate by Capital Cost</button>
            </div>
            <div class="tabs-title tab-weekly">
                <button class="fktab" onclick="openWeekly()">Calculate by Weekly Rental</button>
            </div>
        </div>
        <div class="calculation-tabs-content">
            <div class="visiable-mobile tabs-title active">
                <a href="javascript: void(0)" class="capital-cost-mb" data-tab="capital-cost">Calculate by Capital Cost</a>
            </div>
            <div id="capital-cost" class="calculation-tabs-item active">
                <div class="calculation-calculate">
                    <label>Capital cost</label>
                    <div class="control">
                        <span>$</span><input name="text" id="capital-cost-input" value="" placeholder="0.00">
                    </div>
                    <button type="button" title="Calculate" class="action calculate-capital" onclick="calculateCost('capital-cost')">
                        <span>Calculate</span>
                    </button>
                </div>
